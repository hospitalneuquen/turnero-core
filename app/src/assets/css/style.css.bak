/* Estilos dashboard turnero*/


/* Menú */

.titulo-ventanilla {
    position: absolute;
    width: 100%;
    text-align: center;
    font-size: 2rem;
}


/* layout */

.content {
    padding: 20px;
    height: 100%;
}

.inactivo {
    /*
    -webkit-filter: grayscale(100%);
    */
    /* Safari 6.0 - 9.0 */
    /*
    filter: grayscale(100%);
    */
}

.no-gutters {
    margin-right: 0px;
    margin-left: 0px;
}

.no-gutters>.col,
.no-gutters>[class*="col-"] {
    padding-right: 0px;
    padding-left: 0px;
}

.friso {
    background-color: black;
    color: white;
    height: 80px;
    padding: 10px;
    width: 100%;
}

.friso h1 {
    margin: 0;
    font-size: 3.2em;
}

.friso-lateral {
    background-color: black;
    color: white;
    height: 50px;
    padding: 10px;
    width: 100%;
}

.box {
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    color: #444;
    display: block;
    min-height: calc(100vh - 100px);
    position: relative;
    width: 100%;
}

.box-lateral {
    background-color: white;
    border: solid 1px black;
    height: 100vh;
}

.box-lateral.expandido {
    padding: 20px;
}

.box-turno {
    background-color: white;
    border: solid 8px grey;
    height: auto;
    transition: all 1s ease;
}

.box-turno:hover {
    /*
    -webkit-filter: grayscale(15%);
    */
    /* Safari 6.0 - 9.0 */
    /*
    filter: grayscale(15%);
    */
}

.box-turno:focus {
    /*
    -webkit-filter: grayscale(0%);
    */
    /* Safari 6.0 - 9.0 */
    /*
    filter: grayscale(0%);
    */
}

.box-ventanilla {
    color: grey;
    font-size: 5rem;
    font-weight: 300;
    height: 200px;
    padding: 30px;
    text-align: center;
}

.box {
    opacity: 1;
}

.blink {
    animation: blink-animation 1s steps(5, start) infinite;
    -webkit-animation: blink-animation 1s steps(5, start) infinite;
}

.blink-5 {
    animation: blink-animation 1s steps(5, start) 5;
    -webkit-animation: blink-animation 1s steps(5, start) 5;
}

@keyframes blink-animation {
    to {
        opacity: .2;
    }
}

@-webkit-keyframes blink-animation {
    to {
        opacity: .2;
    }
}

@media screen and (max-width: 1024px) {
    .box-ventanilla {
        font-size: 2.5rem;
        height: 150px;
        padding: 30px;
    }
}


/* Estilo Numeros Monitor */

.cabezal {
    height: 120px;
    width: 100%;
}

.texto-cabezal {
    color: black;
    font-size: 1.75rem;
    font-weight: 600;
    text-transform: uppercase;
    vertical-align: middle;
    padding-left: 10%;
}

.texto-cabezal span {
    text-align: center;
}


/*span.numero-cabezal {
  background-color: black;
