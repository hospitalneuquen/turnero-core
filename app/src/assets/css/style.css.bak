}*/

.numeros-container {
    display: flex;
    justify-content: center;
}

.numero-container {
    min-width: 265px;
    padding: 0 5px 0 10px;
}

span.numero-cabezal {
    background-color: black;
    color: white;
    font-size: 1.75rem;
    position: absolute;
    width: 25%;
    right: 0;
}

.cuerpo {
    background-color: white;
    text-transform: uppercase;
    text-align: center;
    vertical-align: middle;
    position: relative;
    width: 100%;
    /* padding-top: 100px;*/
    /* height: 600px; */
    height: 45vh;
}

.cuerpo>.tipo-turno {
    color: grey;
    font-size: 3.2rem;
    font-weight: 500;
}

@media screen and (max-width: 1024px) {
    .cuerpo>.tipo-turno {
        font-size: 2.5rem;
    }
}

.cuerpo>.numero {
    color: black;
    font-size: 7rem;
    line-height: 7rem;
    font-weight: bold;
    margin: 0 20px 0 20px;
    vertical-align: center;
}

@media screen and (max-width: 1024px) {
    .cuerpo>.numero {
        color: black;
        font-size: 6rem;
        line-height: 6rem;
        font-weight: bold;
        margin: 0 20px 0 20px;
    }
}

.cuerpo>.llamado {
    color: white;
    display: inline-block;
    font-size: 2rem;
    font-weight: 500;
    line-height: 1.5rem;
    margin: 50px 0 0;
    text-align: center;
    padding: 15px;
    vertical-align: middle;
    white-space: nowrap;
    text-shadow: 1px 1px 0px #6a9436;
}

.cuerpo>.llamado.success {
    background-color: #8CC63F;
}

.cuerpo>.llamado.warning {
    background-color: #ff8d22;
}

.cuerpo>.llamado.danger {
    background-color: #dd4b39;
}


/* Estilos dashboard turnos */

.zocalo {
    background-color: black;
    bottom: 0px;
    height: 110px;
    text-align: center;
    vertical-align: middle;
    width: 100%;
    position: absolute;
}

.zocalo.lateral {
    background-color: #a0a0a0;
    bottom: 0px;
}

.zocalo-texto {
    font-size: 1.5rem;
    font-weight: bold;
    line-height: 1.5rem;
    margin-top: 25px;
}

h2.zocaloNumero {
    font-size: 6rem;
    line-height: 6rem;
    font-weight: bold;
}

.border-bottom {
    border-bottom: solid 1px black;
    line-height: 3rem;
    margin-bottom: 30px;
}

.color>input[type="radio"] {
    display: none;
}

.color>input[type="radio"]+label {
    color: #292321;
    font-family: Arial, sans-serif;
    font-size: 14px;
}

.color>input[type="radio"]+label span {
    display: inline-block;
    width: 19px;
    height: 19px;
    margin: -1px 4px 0 0;
    vertical-align: middle;
    cursor: pointer;
    -moz-border-radius: 50%;
    border-radius: 50%;
    height: 60px;
    width: 60px;
}

.color>input[type="radio"]+label span {}

.color>input[type="radio"]:checked+label span {
    outline: solid 2px #00A8E0;
}

.cuadradito:checked {}

.color>input[type="radio"]+label span,
.color>input[type="radio"]:checked+label span {
    -webkit-transition: background-color 0.4s linear;
    -o-transition: background-color 0.4s linear;
    -moz-transition: background-color 0.4s linear;
    transition: background-color 0.4s linear;
}

.cuadradito {
    border: solid 1px grey;
    margin: 0 20px 5px 0;
    height: 60px;
    width: 60px;
}

.cuadradito.amarillo {
    background-color: #FFFECF;
}

.cuadradito.verde {
    background-color: #BEEDCC;
}

.cuadradito.rosado {
    background-color: #FBDED6;
}

.cuadradito.celeste {
    background-color: #B9E5EF;
}

.cuadradito.violeta {
    background-color: #B5BFF4;
}

.cuadradito:hover {
    border: solid 2px #00A8E0;
}

.cuadradito:focus {
    outline: solid 2px #00A8E0;
}


/* Modificados Bootstrap */

.form-control {
    margin-bottom: 20px;
}


/* Botones */

.btn {
    box-shadow: none;
    border-radius: 0px;
    font-size: 0.85rem;
    font-weight: 200;
    text-transform: uppercase;
    /*padding: 20px;*/
}

@media screen and (max-width: 1024px) {
    .btn {
        padding: 10px;
    }
}

.btn-danger {
    box-shadow: none;
    border-radius: 0px;
    border: 0px;
    font-weight: 200;
}

.btn-confirmar {
    background-color: #8CC63F;
    bottom: 0px;
    color: white;
    font-size: 1.15rem;
    margin: 50px 20px 0 20px;
    position: relative;
    text-transform: uppercase;
    width: 100%;
}

.btn-confirmar:hover {
    background-color: #209245;
}

.btn-pausar {
    background-color: #dd4b39;
    color: white;
    font-size: 1.15rem;
    margin: 20px;
    text-transform: uppercase;
    width: 30%;
}

.btn-pausar:hover {
    background-color: #891620;
}

.btn-llamar {
    background-color: #8CC63F;
    color: white;
    display: inline-block;
    font-size: 1.5rem;
    margin: 80px 0;
    text-align: center;
    white-space: nowrap;
}

.btn-llamar:hover {
    background-color: #209245;
}

@media screen and (max-width: 1024px) {
    .btn-llamar {
        font-size: 1rem;
    }
}


/* Iconos */

.flecha {
    cursor: hand;
    -webkit-filter: opacity(15%);
    /* Safari 6.0 - 9.0 */
    filter: opacity(15%);
    position: absolute;
    transition: all 1s ease;
    top: 45%;
}

.flecha.back {
    left: 15%;
    -ms-transform: rotate(180deg);
    /* IE 9 */
    -webkit-transform: rotate(180deg);
    /* Chrome, Safari, Opera */
    transform: rotate(180deg);
}

.flecha.next {
    right: 15%;
}

.flecha:hover {
    -webkit-filter: opacity(100%);
    /* Safari 6.0 - 9.0 */
    filter: opacity(100%);
}

@media screen and (max-width: 1024px) {
    .flecha {
        top: 35%;
    }
}

.campanita {
    background-color: #209245;
    padding: 5px;
}


/* Links*/

a {
    /* color: #111111; */
    color: #00A8E0;
}

a:hover,
a:active,
a:focus {
    outline: 5px;
    /* text-decoration: none; */
}


/* Badges */

.badge {
    background: transparent;
    border: 1.5px solid;
    border-radius: 0px;
    text-transform: uppercase;
    font-size: 0.85rem;
    font-weight: 500;
    margin: 0px 10px 10px 0px;
    width: 100%;
}

.badge.badge-danger {
    border-color: #dd4b39;
    color: #dd4b39;
}

.badge.badge-success {
    border-color: #8CC63F;
    color: #8CC63F;
}

.badge.badge-warning {
    border-color: #ff8d22;
    color: #ff8d22;
}

.badge.badge-info {
    border-color: #00A8E0;
    color: #00A8E0;
}